<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="sale_order_manual_commissions_wizard_view_form" model="ir.ui.view">
        <field name="name">sale.order.manual.commissions.wizard.view.form</field>
        <field name="model">sale.order.manual.commissions.wizard</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="sale_order_id" string="Sales Order" readonly="1" />
                        <field name="partner_agent_ids" widget="one2many_list">
                            <tree editable="bottom">
                                <field name="agent_id" />
                                <field name="commission_id" />
                            </tree>
                        </field>
                    </group>
                </sheet>
                <footer>
                    <button string="Save &amp; Close" class="btn-primary" special="save" />
                </footer>
            </form>
        </field>
    </record>

</odoo>
